<h1>RANDOM BEER</h1>
<div>
    {{#each beer}}
    <div class="col-md-4">
        <div class="card">
            <img src="{{this.image_url}}" alt="{{this.name}}"/>
        <div class="card-body">
            <h5 class="card-title">{{this.name}}</h5>
            <p class="card-text">{{this.description}}</p>
            <p class="card-text">{{this.tagline}}</p>
            <p class="card-text">{{this.food_pairing}}</p>
            <p class="card-text">{{this.brewers_tips}}</p>
        </div>
        </div>
    </div>
    {{/each}}
</div>